{
  "name": "bundle-stats",
  "version": "1.2.3",
  "description": "In-depth reports for webpack bundles",
  "main": "lib/index.js",
  "bin": {
    "bundle-stats": "bin/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">= 8.0"
  },
  "scripts": {
    "build": "babel src -d lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests src "
  },
  "keywords": [
    "webpack",
    "bundle-size",
    "bundle-analyzer",
    "bundle-stats",
    "stats",
    "bundle",
    "size",
    "assets",
    "chunks",
    "modules"
  ],
  "author": {
    "name": "Viorel Cojocaru",
    "email": "vio@beanon.com",
    "url": "https://beanon.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bundle-stats/bundle-stats.git"
  },
  "bugs": {
    "url": "https://github.com/bundle-stats/bundle-stats/issues"
  },
  "homepage": "https://github.com/bundle-stats/bundle-stats/blob/master/packages/cli#readme",
  "dependencies": {
    "@bundle-stats/html-templates": "^1.2.3",
    "@bundle-stats/utils": "^1.2.3",
    "core-js": "^3.1.4",
    "fs-extra": "8.1.0",
    "listr": "^0.14.3",
    "lodash": "4.17.15",
    "yargs": "13.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.12.0",
    "jest": "24.8.0"
  }
}
