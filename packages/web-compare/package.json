{
  "name": "@bundle-stats/web-compare",
  "version": "1.0.0",
  "description": "Side by side comparison for webpack/lighthouse/browsertime stats",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.development.js --progress --hot --mode development",
    "build": "rm -fr ./dist && NODE_ENV=production webpack --config webpack.config.production.js --mode production",
    "storybook": "start-storybook -p 8090 -c storybook",
    "test": "NODE_ENV=test jest --coverage --coverageDirectory ./artifacts/coverage",
    "lint": "eslint . --ext .jsx,.js --cache --cache-location ./node_modules/.eslintcache"
  },
  "keywords": [
    "webpack",
    "lighthouse",
    "browsertime",
    "bundle-size",
    "bundle",
    "assets",
    "chunks",
    "modules",
    "size"
  ],
  "author": {
    "name": "Viorel Cojocaru",
    "email": "vio@beanon.com",
    "url": "https://beanon.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bundle-stats/bundle-stats.git"
  },
  "bugs": {
    "url": "https://github.com/bundle-stats/bundle-stats/issues"
  },
  "homepage": "https://github.com/bundle-stats/bundle-stats/blob/master/packages/web-compare#readme",
  "jest": {
    "transform": {
      "\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@bundle-stats/ui": "^1.0.0",
    "@bundle-stats/utils": "^1.0.0",
    "@moonwalker/orbit-ui": "1.13.3",
    "@storybook/react": "5.0.11",
    "classnames": "^2.2.6",
    "convert-units": "^2.3.4",
    "core-js": "3.1.3",
    "ganalytics": "^3.1.1",
    "lodash": "^4.17.11",
    "preact": "^8.4.2",
    "preact-compat": "3.18.5",
    "preact-helmet": "^4.0.0-alpha-3",
    "preact-router": "^2.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "recompose": "^0.30.0",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@relative-ci/agent": "1.1.1",
    "autoprefixer": "9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-preact-require": "1.0.0",
    "bundle-stats": "^1.0.0",
    "copy-webpack-plugin": "5.0.3",
    "coveralls": "3.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "dotenv": "8.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
