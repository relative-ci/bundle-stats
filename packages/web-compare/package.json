    "postcss-loader": "3.0.0",
    "stats-webpack-plugin": "0.7.0",
    "stylelint": "10.0.1",
    "stylelint-config-css-modules": "1.4.0",
    "stylelint-config-standard": "18.3.0",
    "terser-webpack-plugin": "1.3.0",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.5.1",
    "webpack-merge": "4.2.1"
  }
