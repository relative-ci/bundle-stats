{
  "name": "bundle-stats-webpack-plugin",
  "version": "3.2.2",
  "description": "In-depth bundle analyzer for webpack(bundle size, assets, modules, packages)",
  "main": "lib/index.js",
  "types": "typings.d.ts",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">= 12.0"
  },
  "scripts": {
    "build": "babel src -d lib",
    "lint": "../../node_modules/.bin/eslint . --ext .js",
    "test": "TZ=UTC jest src ",
    "test-package": "TZ=UTC jest test/package/*.test.js"
  },
  "keywords": [
    "webpack",
    "webpack-plugin",
    "bundle-size",
    "bundle-analyzer",
    "bundle-stats",
    "stats",
    "bundle",
    "size",
    "assets",
    "chunks",
    "modules"
  ],
  "author": {
    "name": "Viorel Cojocaru",
    "email": "vio@beanon.com",
    "url": "https://beanon.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relative-ci/bundle-stats.git"
  },
  "bugs": {
    "url": "https://github.com/relative-ci/bundle-stats/issues"
  },
  "homepage": "https://github.com/relative-ci/bundle-stats/blob/master/packages/webpack-plugin#readme",
  "dependencies": {
    "@bundle-stats/cli-utils": "^3.2.2",
    "@bundle-stats/utils": "^3.2.2",
    "core-js": "3.20.0",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@types/jest": "27.0.3",
    "jest": "27.4.5",
    "jest-date-mock": "1.0.8",
    "memory-fs": "0.5.0",
    "webpack": "5.65.0",
    "webpack4": "npm:webpack@4.46.0"
  },
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  }
}
