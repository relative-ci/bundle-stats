{
  "name": "@bundle-stats/ui",
  "version": "1.0.0-rc.0",
  "description": "UI components for BundleStats projects.",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "scripts": {
    "build": "npm run build-lib && npm run build-esm",
    "build-lib": "rm -fr ./lib && babel -D src -d lib --ignore **/__tests__/** --ignore /prototypes/** --presets=@babel/preset-env,@babel/preset-react",
    "build-esm": "rm -fr ./lib-esm && babel -D src -d lib-esm --ignore **/__tests__/** --ignore /prototypes/**",
    "dev": "start-storybook -c ./build/storybook -p 8080",
    "generate-chart-colors": "node scripts/generate-chart-colors.js",
    "lint": "eslint . --ext .jsx,.js",
    "prepublishOnly": "npm run build",
    "test": "npm run test:unit && npm run test:storyshots",
    "test:unit": "jest src",
    "test:storyshots": "jest build/storybook "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bundle-stats/bundle-stats.git"
  },
  "keywords": [],
  "author": {
    "name": "Viorel Cojocaru",
    "email": "vio@beanon.com",
    "url": "https://beanon.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bundle-stats/bundle-stats/issues"
  },
  "homepage": "https://github.com/bundle-stats/bundle-stats/blob/master/packages/ui/README.md",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@bundle-stats/utils": "1.0.0-rc.0",
    "@storybook/addon-knobs": "5.0.6",
    "@storybook/addon-storyshots": "5.0.4",
    "@storybook/addons": "5.0.4",
    "@storybook/react": "5.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.5.0",
    "babel-loader": "8.0.5",
    "babel-plugin-inline-react-svg": "1.0.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-require-context-hook": "1.0.0",
    "classnames": "2.2.6",
    "colormap": "2.3.1",
    "core-js": "3.0.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "hex-rgb": "4.0.0",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.5.0",
    "lodash": "4.17.11",
    "markdown-loader-jest": "0.1.1",
    "prop-types": "15.7.2",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-test-renderer": "16.8.5",
    "recompose": "0.30.0",
    "stats-webpack-plugin": "0.7.0",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0"
  },
  "peerDependencies": {
    "@bundle-stats/utils": "^1.0.0-rc.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
