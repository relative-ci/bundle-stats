{
  "name": "@bundle-stats/ui",
  "version": "3.3.2",
  "description": "UI components for BundleStats projects.",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "scripts": {
    "build": "npm run build-lib && npm run build-esm",
    "prebuild-lib": "rimraf ./lib",
    "build-lib": "babel -D src -d lib --ignore **/__tests__/**/* --ignore /prototypes/** --presets=@babel/preset-env,@babel/preset-react",
    "prebuild-esm": "rimraf ./lib-esm",
    "build-esm": "babel -D src -d lib-esm --ignore **/__tests__/**/* --ignore /prototypes/**",
    "build-storybook": "npx build-storybook -c ./build/storybook",
    "generate-chart-colors": "node scripts/generate-chart-colors.js",
    "generate-svg-sprite": "npx svg-sprite -s --si --no-svg-namespace-classnames src/assets/icons/*.svg && mv symbol/svg/sprite.symbol.svg src/assets/icons.svg",
    "lint": "../../node_modules/.bin/eslint . --ext .jsx,.js",
    "start": "cross-env NODE_ENV=development start-storybook -c ./build/storybook -p 8080",
    "test": "npm run test:unit",
    "test:unit": "jest src --passWithNoTests",
    "test:storyshots": "jest build/storybook ",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relative-ci/bundle-stats.git"
  },
  "keywords": [],
  "author": {
    "name": "Viorel Cojocaru",
    "email": "vio@beanon.com",
    "url": "https://beanon.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relative-ci/bundle-stats/issues"
  },
  "homepage": "https://github.com/relative-ci/bundle-stats/blob/master/packages/ui#readme",
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@simplyianm/babel-plugin-inline-react-svg": "1.2.0",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storyshots": "6.4.19",
    "@storybook/addons": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-require-context-hook": "1.0.0",
    "chromatic": "6.5.3",
    "classnames": "2.3.1",
    "colormap": "2.3.2",
    "core-js": "3.21.1",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "hex-rgb": "5.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-date-mock": "1.0.8",
    "lodash": "4.17.21",
    "postcss": "8.4.12",
    "prop-types": "15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "5.3.0",
    "react-test-renderer": "16.14.0",
    "rimraf": "3.0.2"
  },
  "peerDependencies": {
    "classnames": "^2.0.0",
    "core-js": "^3.0.0",
    "lodash": "^4.0.0",
    "prop-types": "^15.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^5.0.0"
  },
  "dependencies": {
    "@bundle-stats/utils": "^3.3.2",
    "ariakit": "2.0.0-alpha.19",
    "date-fns": "^2.28.0",
    "modern-css-reset": "^1.4.0",
    "query-string": "^7.1.0",
    "use-query-params": "^1.2.3"
  }
}
